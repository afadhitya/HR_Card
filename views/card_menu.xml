<?xml version="1.0"?>
<odoo>
	<!-- Action to open vote_app list -->
	

	<act_window id="action_id_card"
		name="Carding"
		res_model="id.card"
		view_mode="tree,form"/>
	
	<act_window id="action_bussiness_card"
		name="Carding Bussiness"
		res_model="business.card"
		view_mode="tree,form"/>
	
	
	
	<!-- Top Menu -->
	<menuitem id="menu_id_app"
		name="Carding"
		action="action_id_card" 
		/>
	<!-- Menu item to open vote app list / Side Menu-->	
	<menuitem id="side_menu_id_card" 
		name="Id Card"
		parent="menu_id_app"
		/>
	<menuitem id="side_menu_idcard"
		name="Create Id Card"
		action="action_id_card"
		parent="side_menu_id_card"
		/>


	<menuitem id="side_menu_bussiness_card"
		name="Bussines Card"
		parent="menu_id_app"
		
		/>	
	<menuitem id="side_menu_bussinesscard"
		name="Create Bussines Card"
		parent="side_menu_bussiness_card"
		action="action_bussiness_card"
		
		/>
</odoo>